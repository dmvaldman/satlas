{
  "name": "Satlas",
  "version": "1.0.0",
  "description": "A gazetteer of sits",
  "type": "module",
  "keywords": [],
  "scripts": {
    "lint": "eslint",
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "cap:sync": "cap sync",
    "cap:copy": "cap copy",
    "android:build": "npm run build && npm run cap:copy && cap run android",
    "android:clean-build": "npm run build && npm run cap:copy && cd android && ./gradlew clean && cd .. && cap run android",
    "ios:build": "npm run build && npm run cap:copy && cap run ios",
    "ios:clean-build": "npm run build && npm run cap:copy && cd ios && xcodebuild clean && cd .. && cap run ios",
    "deploy:android": "npm run android:build",
    "deploy:android:clean": "npm run android:clean-build",
    "deploy:ios": "npm run ios:build",
    "deploy:ios:clean": "npm run ios:clean-build",
    "dev:android": "npx cap run android --live-reload --port=5173",
    "dev:android-build": "npx cap sync android && cd android && ./gradlew assembleDebug && cd .."
  },
  "main": "index.js",
  "dependencies": {
    "@capacitor-firebase/authentication": "^7.1.0",
    "@capacitor/android": "^7.0.1",
    "@capacitor/app": "^7.0.0",
    "@capacitor/camera": "^7.0.0",
    "@capacitor/cli": "^7.0.1",
    "@capacitor/core": "^7.0.1",
    "@capacitor/filesystem": "^7.0.0",
    "@capacitor/geolocation": "^7.1.1",
    "@capacitor/ios": "^7.0.1",
    "@capacitor/keyboard": "^7.0.0",
    "@capacitor/local-notifications": "^7.0.0",
    "@capacitor/network": "^7.0.0",
    "@capacitor/push-notifications": "^7.0.0",
    "@capacitor/status-bar": "^7.0.0",
    "@types/exif-js": "^2.3.1",
    "@types/node": "^22.13.1",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "buffer-image-size": "^0.6.4",
    "capacitor-native-settings": "^7.0.1",
    "dotenv": "^16.4.7",
    "exif-parser": "^0.1.12",
    "firebase": "^11.2.0",
    "mapbox-gl": "^3.10.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-spring-bottom-sheet": "^3.4.1",
    "typescript": "^5.7.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.22.0",
    "@types/mapbox-gl": "^3.4.1",
